{
  "name": "setup-terraform",
  "version": "1.0.2",
  "private": true,
  "description": "setup terraform action",
  "main": "lib/main.js",
  "scripts": {
    "audit": "npm install && npm audit --audit-level=moderate",
    "build": "tsc",
    "format": "prettier --write **/*.ts",
    "format-check": "prettier --check **/*.ts",
    "lint": "eslint src/**/*.ts",
    "pack": "ncc build",
    "test": "jest",
    "all": "npm run build && npm run format && npm run lint && npm run pack && npm test"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/volcano-coffee-company/setup-terraform.git"
  },
  "keywords": [
    "actions",
    "terraform",
    "setup"
  ],
  "author": "volcano-coffee-company",
  "license": "MIT",
  "dependencies": {
    "@actions/core": "^1.2.1",
    "@actions/exec": "^1.0.3",
    "@actions/github": "^2.0.1",
    "@actions/http-client": "^1.0.1",
    "@actions/io": "^1.0.2",
    "@actions/tool-cache": "^1.3.0",
    "semver": "^7.1.1"
  },
  "devDependencies": {
    "@types/jest": "^24.9.0",
    "@types/node": "^13.1.7",
    "@types/semver": "^6.0.0",
    "@typescript-eslint/parser": "^2.8.0",
    "@zeit/ncc": "^0.21.0",
    "eslint": "^5.16.0",
    "eslint-plugin-github": "^2.0.0",
    "eslint-plugin-jest": "^23.6.0",
    "jest": "^24.9.0",
    "jest-circus": "^24.9.0",
    "js-yaml": "^3.13.1",
    "prettier": "^1.19.1",
    "ts-jest": "^24.3.0",
    "typescript": "^3.7.5"
  }
}
