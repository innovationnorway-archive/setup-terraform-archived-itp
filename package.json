{
  "name": "setup-terraform",
  "version": "1.0.2",
  "private": true,
  "description": "setup terraform action",
  "main": "lib/main.js",
  "scripts": {
    "audit": "npm install && npm audit --audit-level=moderate",
    "build": "tsc",
    "format": "prettier --write **/*.ts",
    "format-check": "prettier --check **/*.ts",
    "lint": "eslint src/**/*.ts",
    "pack": "ncc build",
    "test": "jest",
    "all": "npm run build && npm run format && npm run lint && npm run pack && npm test"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/volcano-coffee-company/setup-terraform.git"
  },
  "keywords": [
    "actions",
    "terraform",
    "setup"
  ],
  "author": "volcano-coffee-company",
  "license": "MIT",
  "dependencies": {
    "@actions/core": "^1.2.3",
    "@actions/exec": "^1.0.4",
    "@actions/github": "^2.1.1",
    "@actions/http-client": "^1.0.8",
    "@actions/io": "^1.0.2",
    "@actions/tool-cache": "^1.3.4",
    "semver": "^7.3.2"
  },
  "devDependencies": {
    "@types/jest": "^25.2.1",
    "@types/node": "^14.0.1",
    "@types/semver": "^7.1.0",
    "@typescript-eslint/parser": "^2.31.0",
    "@zeit/ncc": "^0.22.1",
    "eslint": "^5.16.0",
    "eslint-plugin-github": "^2.0.0",
    "eslint-plugin-jest": "^23.8.2",
    "jest": "^25.5.4",
    "jest-circus": "^26.0.1",
    "js-yaml": "^3.13.1",
    "prettier": "^2.0.4",
    "ts-jest": "^25.4.0",
    "typescript": "^3.8.3"
  }
}
